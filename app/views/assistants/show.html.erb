<div class="main-container">
        <div class="container">
            <div class="row">
                <div class="col-sm-3 page-sidebar">
                    <aside>
                        <div class="inner-box">
                            <div class="user-panel-sidebar">
                                <div class="collapse-box">
                                    

                                <% if current_assistant %>
                                    <div class="panel-collapse collapse in">
                                        <ul class="acc-list">
                                            <li><%= link_to "View My Profile", "/assistants/#{current_assistant.id}", method: :get %></li>
                                            <li><%= link_to "Edit My Profile", "/assistants/#{current_assistant.id}/edit", method: :get %></li>
                                            <li><%= link_to "Back To Assistants", "/assistants", method: :get %></li>
                                            <% else %>
                                            <li><%= link_to "Not a member?", "/assistants/sign_up", method: :get %></li>
                                            <li><%= link_to "Back To Assistants", "/assistants", method: :get %></li>
                                            <% end %>
                                            
                                        </ul>
                                    </div>
                                </div>
                                

                                
                                <!-- /.collapse-box  -->
                            </div>
                        </div>
                        <!-- /.inner-box  -->

                    </aside>
                </div>
                <!--/.page-sidebar-->

                <div class="col-sm-9 page-content">
                    <div class="inner-box">
                        <div class="row">
                            <div class="col-md-5 col-xs-4 col-xxs-12">
                                <h3 class="no-padding text-center-480 useradmin"><a href=""><img class="userImg"
                                alt="user" <%= image_tag (@assistant.avatar.url(:thumb)) %>
                                
                                <%= @assistant.name ? @assistant.name : nil %>
                                </a></h3>
                            </div>
                            <div class="col-md-7 col-xs-8 col-xxs-12">
                                <div class="header-data text-center-xs">

                                    <!-- Traffic data -->
                                    
                                    <!-- revenue data -->
                                    

                                    <!-- revenue data -->
                                    <div class="hdata">
                                        <div class="mcol-left">
                                            <!-- Icon with blue background -->
                                            </div>
                                        <div class="mcol-right">
                                            <!-- Number of visitors -->
                                            
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="inner-box">
                        <div class="welcome-msg">
                            

                        <% if current_assistant && current_assistant.id == @assistant.id %>
                            <h3 class="page-sub-header2 clearfix no-padding">Hello <%= current_assistant.name %> </h3>
                            <% else %>
                            <% end %>
                        </div>

                        <!--/.row-box End-->
                        <div class="row">
                          
                                  <label class="col-sm-3 control-label">Name</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.name %></p>
                                  </div>
                                  
                                  <!-- <label class="col-sm-3 control-label">Company</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.company %></p>
                                  </div> -->
                                  
                                  <label class="col-sm-3 control-label">Email</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.email %></p>
                                  </div>
                                  
                                  <label class="col-sm-3 control-label">Phone Number</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.phone_number %></p>
                                  </div>
                                  
                                  <label class="col-sm-3 control-label">Address</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.address %></p>
                                  </div>
                                  
                                  <label class="col-sm-3 control-label">Day Rate</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.day_rate %></p>
                                  </div>
                                  
                                  <label class="col-sm-3 control-label">Worked With</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.worked_with %></p>
                                  </div>
                                  
                                  <label class="col-sm-3 control-label">URL</label>

                                  <div class="col-sm-9">
                                      <a href="<%= @assistant.url %>"><p><%= @assistant.name %>'s Website</a>
                                  </div>
                                
                                  
                                  <label class="col-sm-3 control-label">Bio</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.bio %></p>
                                  </div>
                                  
                                  <% if @assistant.facebook == nil %>
                                  <% else %>
                                  <label class="col-sm-3 control-label">Facebook</label>

                                  <div class="col-sm-9">
                                      <a href="<%= @assistant.facebook %>"><p><%= @assistant.name %>'s Facebook</a>
                                  </div>
                                  <% end %>
                                  
                                  <% if @assistant.twitter == nil %>
                                  <% else %>
                                  <label class="col-sm-3 control-label">Twitter</label>

                                  <div class="col-sm-9">
                                      <a href="<%= @assistant.twitter %>"><p><%= @assistant.name %>'s Twitter</p></a>
                                  </div>
                                  <% end %>
                                  
                                  <% if @assistant.instagram == nil %>
                                  <% else %>
                                  <label class="col-sm-3 control-label">Instagram</label>

                                  <div class="col-sm-9">
                                      <a href="<%= @assistant.instagram %>"><p><%= @assistant.name %>'s Instagram</p></a>
                                  </div>
                                  <% end %>
                                  
                                  <% if @assistant.blog == nil %>
                                  <% else %>
                                  <label class="col-sm-3 control-label">Blog</label>

                                  <div class="col-sm-9">
                                      <a href="<%= @assistant.blog %>"><p><%= @assistant.name %>'s Blog </p></a>
                                  </div>
                                  <% end %>

                                  <label class="col-sm-3 control-label">Years of Experience</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.years_exp %></p>
                                  </div>

                                  <label class="col-sm-3 control-label">Occupation</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.occupation.name %></p>
                                  </div>

                                  <label class="col-sm-3 control-label">Car</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.car %></p>
                                  </div>

                                  <label class="col-sm-3 control-label">City</label>

                                  <div class="col-sm-9">
                                      <p><%= @assistant.city.name %></p>
                                  </div>

                                  <label class="col-sm-3 control-label">Skills</label>

                                  <div class="col-sm-9">
                                      <ul>
                                          <% @assistant.skills.each do |skill| %>
                                          <li><%= skill.name %></li>
                                          <% end %>
                                          </ul>
                                  </div>

                                  <label class="col-sm-3 control-label">Genre</label>

                                  <div class="col-sm-9">
                                      <ul>
                                          <% @assistant.categories.each do |category| %>
                                          <li><%= category.name %></li>
                                          <% end %>
                                          </ul>
                                  </div>

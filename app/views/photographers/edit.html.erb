<div class="main-container">
        <div class="container">
            <div class="row">
                <div class="col-sm-3 page-sidebar">
                    <aside>
                        <div class="inner-box">
                            <div class="user-panel-sidebar">
                                <div class="collapse-box">
                                    

                                    <div class="panel-collapse collapse in">
                                        <ul class="acc-list">
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                View Profile </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}/edit"><i class="icon-home"></i>
                                                Edit Profile </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                View Jobs </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                Edit Jobs </a></li>

                                        </ul>
                                    </div>
                                </div>
                                

                                <div class="collapse-box">
                                    <h5 class="collapse-title"> Terminate Account <a href="#TerminateAccount"
                                                                                     data-toggle="collapse"
                                                                                     class="pull-right"><i
                                            class="fa fa-angle-down"></i></a></h5>

                                    <div class="panel-collapse collapse in" id="TerminateAccount">
                                        <ul class="acc-list">
                                            <li><a href="account-close.html"><i class="icon-cancel-circled "></i> Close
                                                account </a></li>
                                        </ul>
                                    </div>
                                </div>
                                <!-- /.collapse-box  -->
                            </div>
                        </div>
                        <!-- /.inner-box  -->

                    </aside>
                </div>
                <!--/.page-sidebar-->

                <div class="col-sm-9 page-content">
                    <div class="inner-box">
                        <div class="row">
                            <div class="col-md-5 col-xs-4 col-xxs-12">
                                <h3 class="no-padding text-center-480 useradmin"><a href=""><img class="userImg"
                                alt="user" <%= image_tag("user.jpg") %>
                                
                                <%= @photographer.name ? @photographer.name : nil %>
                                </a></h3>
                            </div>
                            <div class="col-md-7 col-xs-8 col-xxs-12">
                                <div class="header-data text-center-xs">

                                    <!-- Traffic data -->
                                    <div class="hdata">
                                        <div class="mcol-left">
                                            <!-- Icon with red background -->
                                            </div>
                                        <div class="postadd">
                                            <!-- Number of visitors -->
                                            <p><a class="btn btn-block   btn-border btn-post btn-danger"
                                               href="job-post.html">Post A Job</a></p>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>

                                  

                                    <!-- revenue data -->
                                    <div class="hdata">
                                        <div class="mcol-left">
                                            <!-- Icon with blue background -->
                                            </div>
                                        <div class="mcol-right">
                                            <!-- Number of visitors -->
                                            
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="inner-box">
                        <div class="welcome-msg">
                            <h3 class="page-sub-header2 clearfix no-padding">Hello <%= @photographer.name %> </h3>
                        </div>

                        <!--/.row-box End-->
                        <div class="row">
                          <%= form_tag "/photographers/#{@photographer.id}", method: :patch do %>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Name</label>

                                  <div class="col-sm-9">
                                      <input name="name" type="string" class="form-control" value="<%= @photographer.name %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Company</label>

                                  <div class="col-sm-9">
                                      <input name="company" type="string" class="form-control" value="<%= @photographer.company %>">
                                  </div>
                              </div>
                              <!-- <div class="form-group">
                                  <label class="col-sm-3 control-label">Email</label>

                                  <div class="col-sm-9">
                                      <input name="email" type="string" class="form-control" value="<%= @photographer.email %>">
                                  </div>
                              </div> -->
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Mobile Number</label>

                                  <div class="col-sm-9">
                                      <input name="mobile_number" type="string" class="form-control" value="<%= @photographer.mobile_number %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label for="Phone" class="col-sm-3 control-label">Studio Number</label>

                                  <div class="col-sm-9">
                                      <input name="studio_number" type="string" class="form-control" value="<%= @photographer.studio_number %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Address</label>

                                  <div class="col-sm-9">
                                      <input name="address" type="string" class="form-control" value="<%= @photographer.address %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Clients</label>

                                  <div class="col-sm-9">
                                      <input name="clients" type="text" class="form-control" value="<%= @photographer.clients %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">URL</label>

                                  <div class="col-sm-9">
                                      <input name="url" type="string" class="form-control" value="<%= @photographer.url %>">
                                  </div>
                              </div>
                              <!-- <div class="form-group">
                                  <label class="col-sm-3 control-label">User Photo</label>

                                  <div class="col-sm-9">
                                      <input name="user_photo" type="string" class="form-control" value="<%= @photographer.user_photo %>">
                                  </div>
                              </div> -->
                             

                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Bio</label>

                                  <div class="col-sm-9">
                                      <input name="bio" type="text" class="form-control" value="<%= @photographer.bio %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Facebook</label>

                                  <div class="col-sm-9">
                                      <input name="facebook" type="string" class="form-control" value="<%= @photographer.facebook %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Twitter</label>

                                  <div class="col-sm-9">
                                      <input name="twitter" type="string" class="form-control" value="<%= @photographer.twitter %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Instagram</label>

                                  <div class="col-sm-9">
                                      <input name="instagram" type="string" class="form-control" value="<%= @photographer.instagram %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Blog</label>

                                  <div class="col-sm-9">
                                      <input name="blog" type="string" class="form-control" value="<%= @photographer.blog %>">
                                  </div>
                              </div>
                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Years of Experience</label>

                                  <div class="col-sm-9">
                                      <input name="years_exp" type="text" class="form-control" value="<%= @photographer.years_exp %>">
                                  </div>
                              </div>

                              <div class="form-group">
                                  <label class="col-sm-3 control-label">Occupation</label>
                              <%= select_tag "occupations", options_from_collection_for_select(@occupations, "id", "name") %>
                              </div>

                              <div class="form-group">
                                  <label class="col-sm-3 control-label">City</label>
                                  <%= select_tag "cities", options_from_collection_for_select(@cities, "id", "name") %>
                              </div>

                              <div class="form-group">
                                  <div class="col-sm-offset-3 col-sm-9">
                                      <button type="submit" class="btn btn-default">Update</button>
                                  </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>

                            <% end %>
                        </div>
                    </div>
                </div>
                <!--/.page-content-->
            </div>
            <!--/.row-->
        </div>
        <!--/.container-->
    </div>
    <!-- /.main-container -->
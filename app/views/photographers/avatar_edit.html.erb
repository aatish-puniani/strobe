<div class="main-container">
        <div class="container">
            <div class="row">
                <div class="col-sm-3 page-sidebar">
                    <aside>
                        <div class="inner-box">
                            <div class="user-panel-sidebar">
                                <div class="collapse-box">
                                    

                                    <div class="panel-collapse collapse in">
                                        <ul class="acc-list">
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                View Profile </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}/edit"><i class="icon-home"></i>
                                                Edit Profile </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                View Jobs </a></li>
                                            <li><a class="active" href="/photographers/#{@photographer.id}"><i class="icon-home"></i>
                                                Edit Jobs </a></li>

                                        </ul>
                                    </div>
                                </div>
                                

                                <div class="collapse-box">
                                    <h5 class="collapse-title"> Terminate Account <a href="#TerminateAccount"
                                                                                     data-toggle="collapse"
                                                                                     class="pull-right"><i
                                            class="fa fa-angle-down"></i></a></h5>

                                    <div class="panel-collapse collapse in" id="TerminateAccount">
                                        <ul class="acc-list">
                                            <li><a href="account-close.html"><i class="icon-cancel-circled "></i> Close
                                                account </a></li>
                                        </ul>
                                    </div>
                                </div>
                                <!-- /.collapse-box  -->
                            </div>
                        </div>
                        <!-- /.inner-box  -->

                    </aside>
                </div>
                <!--/.page-sidebar-->

                <div class="col-sm-9 page-content">
                    <div class="inner-box">
                        <div class="row">
                            <div class="col-md-5 col-xs-4 col-xxs-12">
                                <h3 class="no-padding text-center-480 useradmin"><a href=""><img class="userImg"
                                alt="user" <%= image_tag("user.jpg") %>
                                
                                <%= @photographer.name ? @photographer.name : nil %>
                                </a></h3>
                            </div>
                            <div class="col-md-7 col-xs-8 col-xxs-12">
                                <div class="header-data text-center-xs">

                                    <!-- Traffic data -->
                                    <div class="hdata">
                                        <div class="mcol-left">
                                            <!-- Icon with red background -->
                                            <i class="fa fa-eye ln-shadow"></i></div>
                                        <div class="mcol-right">
                                            <!-- Number of visitors -->
                                            <p><a href="#">Post a Job</a></p>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>

                                  

                                    <!-- revenue data -->
                                    <div class="hdata">
                                        <div class="mcol-left">
                                            <!-- Icon with blue background -->
                                            </div>
                                        <div class="mcol-right">
                                            <!-- Number of visitors -->
                                            
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="inner-box">
                        <div class="welcome-msg">
                            <h3 class="page-sub-header2 clearfix no-padding">Hello <%= @photographer.name %> </h3>
                        </div>

                        <!--/.row-box End-->
                        <div class="row">
                          <%= form_tag "/photographers/#{@photographer.id}", multipart: true, method: :patch do %>
                              
                              
                              <%= file_field_tag :avatar %>

                              
                              <div class="form-group">
                                  <div class="col-sm-offset-3 col-sm-9">
                                      <button type="submit" class="btn btn-default">Update</button>
                                  </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>

                            <% end %>
                        </div>
                    </div>
                </div>
                <!--/.page-content-->
            </div>
            <!--/.row-->
        </div>
        <!--/.container-->
    </div>
    <!-- /.main-container -->